            <if test="score != null">score,</if>
            <if test="comment != null">comment,</if>
            <if test="ifShow != null"> if_show,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="itemId != null">#{itemId},</if>
            <if test="predictStartTime != null">#{predictStartTime},</if>
            <if test="finishTime != null">#{finishTime},</if>
            <if test="ifTimely != null">#{ifTimely},</if>
            <if test="lastingTime != null">#{lastingTime},</if>
            <if test="totalSetTime != null">#{totalSetTime},</if>
            <if test="score != null">#{score},</if>
            <if test="comment != null">#{comment},</if>
            <if test="ifShow != null"> '1',</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
        </trim>
    </insert>
</mapper>